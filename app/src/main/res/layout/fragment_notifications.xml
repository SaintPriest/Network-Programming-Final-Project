<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contributeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context=".ui.notifications.NotificationsFragment" >

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"/>

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"/>

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/material_switch9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9" />

</LinearLayout>